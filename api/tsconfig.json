{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": "src",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "paths": {
      "@dbConfig/*":["./src/db/*"],
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
  ]
}